{
    "manifest_version": 3,
    "name": "Web Time Tracker",
    "version": "1.0",
    "description": "Track time spent on websites and view insights.",
    "permissions": ["tabs", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "128": "icons/icon.png"
        }
    },
    "icons": {
        "128": "icons/icon.png"
    }
}